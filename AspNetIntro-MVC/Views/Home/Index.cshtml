@using AspNetIntro_MVC.ViewModels
@model HomeVM

<h1>@Model.Text</h1>
<h1>@Model.AgeOfAykhan</h1>

<ul>
    @foreach (var item in Model.Students)
    {
        <li>@item</li>
        
    }
</ul>


<ul>
    @for (int i = 0; i < Model.Students.Count; i++)
    { 
        <li>@Model.Students[i]</li>
    }
</ul>

@foreach (var item in Model.Users)
{
    <h3>@(item.Id + "-" +item.Age)</h3>
}





<h3>task1</h3>


<table>
    @foreach (var item in Model.Studentes)
    {
        <tr>
            <td> Name:
                @(item.name)
            </td>
            <td>
                Surname:
                @(item.Surname)
            </td>
            <td>
                Email:
                @(item.Email)
            </td>
            <td>
                Age:
                @(item.Age)
            </td>
        </tr>
        
    }
</table>



